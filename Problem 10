import numpy as np
import matplotlib.pyplot as plt
def f(x,y):
    return 1/((y-x*y)**2 + x**2)
h=0.001
n=101
x=np.linspace(0,1,n)
y=np.zeros(n, dtype=float)
y[0]=1
for i in range(0,n-1):
    k1=h*f(x[i],y[i])
    k2=h*f(x[i]+h/2,y[i]+k1/2)
    k3=h*f(x[i]+h/2,y[i]+k2/2)
    k4=h*f(x[i]+h,y[i]+k3)
    y[i+1]= y[i]+ (k1+2*k2+2*k3+k4)*(1/6)  
plt.plot(x,y)
print("y(x=1)= ",y[n-1] )
